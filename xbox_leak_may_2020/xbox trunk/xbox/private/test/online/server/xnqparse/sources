    $(WSDK_LIB_PATH)\kernel32.lib \
    $(WSDK_LIB_PATH)\nmapi.lib    \
    $(WSDK_LIB_PATH)\wsock32.lib

INCLUDES=\
    $(INCLUDES); \
    $(BASEDIR)\private\test\inc; \
    $(WSDK_INC_PATH);\
    $(BASEDIR)\private\online\server\xqserver;\
    $(BASEDIR)\private\online\server\servhlp

