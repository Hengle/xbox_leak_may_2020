         $(BASEDIR)\private\test\inc; \
         $(BASEDIR)\private\inc; \
         $(BASEDIR)\private\inc\crypto; \
         $(SDK_INC_PATH); \
         ..\xtestlib; \
         ..\harness

TARGETLIBS=$(SDK_LIB_PATH)\kernel32.lib \
           $(SDK_LIB_PATH)\msvcrt.lib

