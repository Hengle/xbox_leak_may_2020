TARGETNAME=timerw32
TARGETPATH=$(BASEDIR)\private\test\lib
TARGETTYPE=LIBRARY
INCLUDES=$(SDK_INC_PATH);$(BASEDIR)\private\test\inc
SOURCES=timer.c
MSC_WARNING_LEVEL=/W3 /WX
