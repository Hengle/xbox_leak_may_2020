//
// FARPROC type
//

        // (FARPROC)NULL
#define FARPROC_NULL                4


//
// FLOAT type
//
        // 1.0
#define FLOAT_ONE                   1

        // 0.0
#define FLOAT_ZERO                  2


//
// LPFILETIME type
//

        // valid pointer to filetime for return info
#define LPFILETIME_VALID_ADDRESS    1

        // null pointer
#define LPFILETIME_NULL             2

        // valid input data
#define LPFILETIME_VALID_DATA       3


//
// PFLOAT type
//

        // null pointer that should be a pfloat
#define PFLOAT_NULL                 1

        // array[5] of max float
#define PFLOAT_MAX_FLOAT            2

        // array[5] flt_min
#define PFLOAT_MIN_FLOAT            3

        // array[65]
#define PFLOAT_65                   4

        // random
#define PFLOAT_RANDOM               5

        // (PFLOAT)-1
#define PFLOAT_MINUS_ONE            6
