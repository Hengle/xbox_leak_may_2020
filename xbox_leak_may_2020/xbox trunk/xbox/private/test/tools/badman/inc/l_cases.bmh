//
// LONG type
//

        // long value of one k
#define LONG_ONE_K                      1

        // long value of -1
#define LONG_MINUS_ONE                  2

        // long ptr to a valid ANSI string
#define LONG_VALID_STRINGA              3

        // long ptr to a valid UNICODE string
#define LONG_VALID_STRINGW              4

        // zero value
#define LONG_ZERO                       25


//
// LPLOGPALETTE type
//

        // Null LPLOGPALETTE
#define LPLOGPALETTE_NULL           1

        // Allocate LPLOGPALETTE
#define LPLOGPALETTE_VALID          2

        // One palette entry
#define LPLOGPALETTE_ONE            3


//
// LPLONG type
//

        // Null
#define LPLONG_NULL                 1
