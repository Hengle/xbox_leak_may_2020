//
// LPRECT type
//

        // NULL LPRECT
#define LPRECT_NULL             1

        // pointer to a RECT of ([0,0], [100,100])
#define LPRECT_0_0_100_100      2

        // pointer to a RECT of ([100,50], [150,100])
#define LPRECT_100_50_150_100   3

        // pointer to a non well-ordered RECT
#define LPRECT_NOT_WELL_ORDERED 4

        // pointer to an empty RECT
#define LPRECT_EMPTY            5

        // pointer to a RECT of ([20,20], [40,40])
#define LPRECT_20_20_40_40      6

        // random LPRECT value/hard to debug
#define LPRECT_RANDOM           7

        // pointer to a RECT of ([INT_MIN,INT_MIN], [INT_MAX,INT_MAX])
#define LPRECT_MAX              8

        // pointer to a RECT of [(0xf8000000,0xf8000000),(0x08000000, 0x08000000)]
#define LPRECT_MAXRGN           9

        // (LPRECT) -1
#define LPRECT_MINUS_ONE        10


//
// LPRGBQUAD type
//

        // (LPRGBQUAD)NULL
#define LPRGBQUAD_NULL          1

        // (LPRGBQUAD)Rand()
#define LPRGBQUAD_RANDOM        2

        // LPRGBQUAD filled in
#define LPRGBQUAD_SIMPLE        3

        // (LPRGBQUAD) that's big enough
#define LPRGBQUAD_VALID         4

        // (LPRGBQUAD)-1
#define LPRGBQUAD_MINUS_ONE     5
