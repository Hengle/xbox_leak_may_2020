//
// PGENERIC_MAPPING type
//

        // a valid generic mapping
#define PGENERIC_MAPPING_VALID      1

        // NULL generic mapping
#define PGENERIC_MAPPING_NULL       2

        // an invalid pointer
#define PGENERIC_MAPPING_INVALID    3

        // frist DWORD of generic mapping valid
#define PGENERIC_MAPPING_INVALID_1  4

        // frist & second DWORD of generic mapping valid
#define PGENERIC_MAPPING_INVALID_2  5

        // all but last DWORD of generic mapping valid
#define PGENERIC_MAPPING_INVALID_3  6

        // an unaligned pointer to a generic mapping
#define PGENERIC_MAPPING_MALALIGNED 7
