//
// LPVOID type
//

		// Valid non-64k aligned virtual address
#define LPVOID_NONALIGNED_V_ADDR        1

		// Valid 64k aligned virtual address
#define LPVOID_ALIGNED_V_ADDR           2

		// Virtual address in a reserved uncommitted region
#define LPVOID_RES_UNCOMMIT_V_ADDR      3

		// Virtual address of the start of a valid region
#define LPVOID_REGION_BEG_V_ADDR        4

		// Virtual address of a locked 100 byte block
#define LPVOID_LOCKED_V_ADDR            5

		// Virtual address of an unlocked 100 byte block
#define LPVOID_UNLOCKED_V_ADDR          6

		// Null pointer
#define LPVOID_NULL                     7

        // pointer to valid readwrite buffer
#define LPVOID_READWRITE_VALID_BUFFER   15
        
        // pointer to valid version info buffer
#define LPVOID_VER_VALID_INFO_BUFFER    16

        // pointer to valid version info block
#define LPVOID_VER_VALID_INFO_BLOCK     17

        // pointer to invalid version info block
#define LPVOID_VER_INVALID_INFO_BLOCK   18

	    // an invalid pointer
#define	LPVOID_INVALID			        19

	    // a pointer aligned on a 1 byte boundary only
#define	LPVOID_MALALIGNED		        20

	    // a simple 5k Block
#define	LPVOID_VALID		            21

	    // sizeof fileinfo
#define	LPVOID_FILE_INFO                22
