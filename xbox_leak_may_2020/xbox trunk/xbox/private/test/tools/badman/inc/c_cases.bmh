//
// CHAR type
//

        // 'A'
#define CHAR_A      0


//
// COLORREF type
//

        // All blue
#define COLORREF_BLUE      1

        // All red
#define COLORREF_RED       2

        // All Green
#define COLORREF_GREEN     3

        // Value in the top stuff
#define COLORREF_BAD_TOP   4

        // All 0x00ffffff
#define COLORREF_BLACK     5

        // All 0x00000000
#define COLORREF_WHITE     6


//
// LPCONTEXT type
//

        // Pointer to valid context structure
#define LPCONTEXT_VALID_STRUCTURE   1

        // Random pointer value
#define LPCONTEXT_RANDOM_POINTER    2

        // Pointer to NULL
#define LPCONTEXT_NULL_POINTER      3

        // Pointer to block of memory too small to hold context structure
#define LPCONTEXT_BLOCK_TOO_SMALL   4

        // Invalid context flags
#define LPCONTEXT_INVALID_FLAGS     5


//
// LPCRITICAL_SECTION
//

        // Null LPCRITICAL_SECTION
#define LPCRITICAL_SECTION_NULL             0

        // Initialized LPCRITICAL_SECTION
#define LPCRITICAL_SECTION_INITIALIZED      1

        // Uninitialized LPCRITICAL_SECTION
#define LPCRITICAL_SECTION_UNINITIALIZED    2

        // LPCRITICAL_SECTION = 0xffffffff
#define LPCRITICAL_SECTION_MINUS_ONE        3


//
// LPCOLORREF type
//

        // Null COLORREF
#define LPCOLORREF_NULL             1

        // Valid COLORREF
#define LPCOLORREF_VALID            2

        // (LPCOLORREF)-1
#define LPCOLORREF_MINUS_ONE        3
