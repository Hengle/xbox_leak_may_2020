TARGETNAME=xetest
TARGETPATH=obj
TARGETTYPE=PROGRAM
UMTYPE=xboxapp

XE_FLAGS=/TESTID:0x36e6a7d7 /INITFLAGS:3 /TESTNAME:"Xbox XBE Loader Test" /NOPRELOAD:SECTN*

SOURCES=xetest.cpp

i386_SOURCES=sections.asm

INCLUDES=$(SDK_INC_PATH); \
         $(PRIVATE_INC_PATH); \
         $(BASEDIR)\private\ntos\inc; \
         $(BASEDIR)\private\test\inc; \
         $(BASEDIR)\private\ntos\xapi\inc

PRECOMPILED_INCLUDE=xetestp.h
PRECOMPILED_CXX=1

# warning LNK4072: section count XXX exceeds max (96); image may not run
LINKER_FLAGS=$(LINKER_FLAGS) /IGNORE:4072

MSC_WARNING_LEVEL=/W4 /WX
USE_MAPSYM=1
