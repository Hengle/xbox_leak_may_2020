C_DEFINES= $(C_DEFINES) -DUNICODE -D_UNICODE

TARGETNAME=dpserv
TARGETTYPE=DYNLINK

TARGETPATH=obj

DLLDEF=dpserv.def
DLLENTRY=DllMain

UMTYPE=windows

TARGETLIBS=                          \
        $(BASEDIR)\private\test\lib\*\netsync_nt.lib \
        $(LIBRARY_PATH)\kernel32.lib                 \
        $(LIBRARY_PATH)\ntdll.lib                    \
        $(LIBRARY_PATH)\ws2_32.lib

INCLUDES= \
        $(INCLUDES);                              \
        $(SDK_INC_PATH);                          \
        $(BASEDIR)\private\test\inc;              \
        ..\inc;                                   \


SOURCES=                                          \
     	dpserv.rc                                \
        dpserv.cpp

