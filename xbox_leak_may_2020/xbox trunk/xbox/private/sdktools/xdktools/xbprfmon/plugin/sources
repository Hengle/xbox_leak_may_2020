TARGETNAME=xbprfdll
TARGETPATH=obj
TARGETTYPE=DYNLINK

UMTYPE=windows

DLLDEF=xbprfdll.def
DLLENTRY=_DllMainCRTStartup

USE_MSVCRT=1
C_DEFINES=$(C_DEFINES) -D_UNICODE -DUNICODE -DPRFDATA_COLLECT_SUPPORTED

SOURCES=xbprfdll.rc \
        xbprfdll.cpp \
        Optex.cpp \
        PrfData.cpp \
        ..\PrfDataMap.cpp

INCLUDES=$(WSDK_INC_PATH); \
         ..

TARGETLIBS=$(WSDK_LIB_PATH)\advapi32.lib \
           $(WSDK_LIB_PATH)\kernel32.lib \
           $(WSDK_LIB_PATH)\user32.lib

PRECOMPILED_INCLUDE=..\xbprfmonp.h
PRECOMPILED_CXX=1

MSC_WARNING_LEVEL=/W4 /WX

