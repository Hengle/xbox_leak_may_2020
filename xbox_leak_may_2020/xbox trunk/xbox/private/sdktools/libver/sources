TARGETNAME=libver
TARGETPATH=obj
TARGETTYPE=PROGRAM

USE_MSVCRT=1

UMTYPE=console

MSC_WARNING_LEVEL=/W4 /WX

TARGETLIBS=$(LIBRARY_PATH)\user32.lib \
           $(LIBRARY_PATH)\shlwapi.lib

SOURCES=libver.cpp    \
        stringrc.cpp  \
        libver.rc

PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=libverp.h
PRECOMPILED_PCH=libverp.pch
PRECOMPILED_OBJ=libverp.obj

