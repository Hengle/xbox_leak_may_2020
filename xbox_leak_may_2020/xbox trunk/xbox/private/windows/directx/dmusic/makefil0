!IFNDEF UNDEFINED
# -- this method of exporting include files is not used on XBOX

clean:

!ELSE

!INCLUDE $(NTMAKEENV)\makefile.plt

SDKLIB = $(_NTDRIVE)$(_NTROOT)\public\sdk\lib
SDKINC = $(_NTDRIVE)$(_NTROOT)\public\sdk\inc
OAKINC = $(_NTDRIVE)$(_NTROOT)\public\oak\inc
!IFNDEF NTOSINC
NTOSINC = $(_NTDRIVE)$(_NTROOT)\private\ntos\inc
!ENDIF

all:  copyfiles binplace

clean: cleansrc  all

cleansrc:


copyfiles:
	xcopy .\includes\*.* $(SDKINC)

binplace:
	-binplace .\includes\*.*

!ENDIF