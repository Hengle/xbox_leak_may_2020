<script language=vbscript directmusicversion=8.0.0.0>

sub TRoutine
	set scr = LoadScript("script1other.scr")
	scr.TPlay
end sub

sub TSet
	set scr = LoadScript("script1other.scr")
	scr.VGroove = -100
end sub

sub TGet
	set scr = LoadScript("script1other.scr")
	set seg = scr.OSeg
	PlaySegment seg
end sub

sub TJScript
	set scr = LoadScript("script1jscript.scr")
	set scr2 = LoadScript("A Script Using JScript Language")
	scr2.TPlay
end sub
