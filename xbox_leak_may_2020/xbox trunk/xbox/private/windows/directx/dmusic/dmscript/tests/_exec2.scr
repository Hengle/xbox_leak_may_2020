<script language=LimitedVBScript directmusicversion=8.0.0.0>

dim vCool_Text_Exclamations
dim vIs

sub Asgn
	vCool_Text_Exclamations = "Cool Text!!!!!"
end sub

sub IsYes
	set seg1 = LoadSegment("sample.sgt")
	set seg2 = LoadSegment("sample.sgt")
	vIs = seg1 is seg2
end sub

sub IsNo
	set seg1 = LoadSegment("sample.sgt")
	ClearCache
	set seg2 = LoadSegment("sample.sgt")
	vIs = seg1 is seg2
end sub
