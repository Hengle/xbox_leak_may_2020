MSC_OPTIMIZATION =  $(MSC_OPTIMIZATION) /Od

TARGETPATH = obj
TARGETTYPE = PROGRAM
UMTYPE     = xboxapp

INCLUDES   = $(INCLUDES);..\..\common\include

LINKLIBS   = \
             ..\..\common\src\obj\*\xbframe.lib \
             $(XDK_LIB_PATH)\dsound$(D).lib \
             $(XDK_LIB_PATH)\d3d8$(D).lib \
             $(XDK_LIB_PATH)\d3dx8$(D).lib \
             $(XDK_LIB_PATH)\xgraphics$(D).lib


