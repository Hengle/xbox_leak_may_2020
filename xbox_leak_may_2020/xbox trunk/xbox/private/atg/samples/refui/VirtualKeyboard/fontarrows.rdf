// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         FontArrows.h
out_packedresource Media\FontArrows.xpr
out_error          FontArrows.err


Texture Font
{
   Source      Media\Fonts\Arrows.tga
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile Media\Fonts\Arrows.abc
}

