// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         Xboxdings_24.h
out_packedresource Media\Xboxdings_24.xpr
out_error          Media\Xboxdings_24.err


Texture Font
{
   Source       Media\Fonts\Xboxdings_24.tga
   Format       D3DFMT_A4R4G4B4
   Levels       1
}

UserData FontData
{
   DataFile     Media\Fonts\Xboxdings_24.abc
}

