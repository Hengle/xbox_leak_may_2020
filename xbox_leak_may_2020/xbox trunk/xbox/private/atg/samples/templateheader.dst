define(`RESOURCEFILEENTRY',POUND Begin Source File

SOURCE=.\$1.rdf

!IF  "$(CFG)" == "PROJNAME - Xbox Release"

POUND Begin Custom Build
ProjDir=.
InputPath=.\$1.rdf
InputName=$1

BuildCmds= \
    bundler $(InputPath)

"$(ProjDir)\Media\$(InputName).xpr" : $(SOURCE) "$(INTDIR)" "$(OUTDIR)"
   $(BuildCmds)

"$(ProjDir)\$(InputName).h" : $(SOURCE) "$(INTDIR)" "$(OUTDIR)"
   $(BuildCmds)
# End Custom Build

