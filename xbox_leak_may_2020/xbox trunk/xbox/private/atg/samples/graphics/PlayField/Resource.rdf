// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         Resource.h
out_packedresource Media\Resource.xpr
out_error          Resource.err
out_prefix         Resource

Texture Fairway
{
   Source Media\Textures\Fairway.bmp
   AlphaSource Media\Textures\Fairway_alpha.bmp
   Format D3DFMT_A8R8G8B8
   Levels 0
}

Texture Football
{
   Source Media\Textures\Football.bmp
   Format D3DFMT_A8R8G8B8
   Levels 0
}

Texture GrassStrip
{
   Source Media\Textures\GrassStrip.bmp
   AlphaSource Media\Textures\GrassStrip_alpha.bmp
   Format      D3DFMT_A8R8G8B8
   Levels 0
}

Texture Grass
{
   Source Media\Textures\Grass.bmp
   Format D3DFMT_DXT1
   Levels 0
}
