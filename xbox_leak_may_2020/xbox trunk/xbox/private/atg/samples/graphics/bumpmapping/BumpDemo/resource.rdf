// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         Resource.h
out_packedresource Media\Resource.xpr
out_error          Resource.err
out_prefix         resource

Cubemap Lobby
{
   SourceXP Media\Textures\LobbyXP.bmp
   SourceXN Media\Textures\LobbyXN.bmp
   SourceYP Media\Textures\LobbyYP.bmp
   SourceYN Media\Textures\LobbyYN.bmp
   SourceZP Media\Textures\LobbyZP.bmp
   SourceZN Media\Textures\LobbyZN.bmp
   Format   D3DFMT_A8R8G8B8
   Size     256
}

Texture GearBump
{
   Source      Media\Textures\GearBump.bmp
   Format      D3DFMT_A8R8G8B8
}

Texture Gear
{
   Source Media\Textures\Gear.bmp
   Format      D3DFMT_R5G6B5
}

