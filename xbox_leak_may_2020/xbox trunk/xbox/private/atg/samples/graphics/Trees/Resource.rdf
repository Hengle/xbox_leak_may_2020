// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         Resource.h
out_packedresource Media\Resource.xpr
out_error          Resource.err
out_prefix         resource

Texture TerrainTexture
{
   Source Media\Textures\land1.bmp
   Format D3DFMT_A8R8G8B8
   Filter TRIANGLE|CLAMP
   Width  256
   Height 256
   Levels 8
}

Texture undergrass
{
   Source Media\Textures\undergrass.bmp
   Format D3DFMT_A8R8G8B8
   Filter TRIANGLE|WRAP
   Levels 0
}

Texture leaf2
{
   Source Media\Textures\leaf2.tga
   Format D3DFMT_A8R8G8B8
   Filter TRIANGLE|CLAMP
   Levels 0
}
