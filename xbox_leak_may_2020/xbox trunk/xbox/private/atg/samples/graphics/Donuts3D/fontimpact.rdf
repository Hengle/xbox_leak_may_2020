// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         FontImpact.h
out_packedresource Media\FontImpact.xpr
out_error          FontImpact.err


Texture Font
{
   Source      Media\Fonts\Impact_48.tga
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile Media\Fonts\Impact_48.abc
}

