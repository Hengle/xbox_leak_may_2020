// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         FontTahoma.h
out_packedresource Media\FontTahoma.xpr
out_error          FontTahoma.err


Texture Font
{
   Source      Media\Fonts\Tahoma_30.tga
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile Media\Fonts\Tahoma_30.abc
}

