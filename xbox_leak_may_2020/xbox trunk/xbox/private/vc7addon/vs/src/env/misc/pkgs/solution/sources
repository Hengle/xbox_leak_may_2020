!include $(_NTROOT)\private\vc7addon\vsenv.inc

USE_NATIVE_EH=1
386_STDCALL=0

!include $(VSROOT)\sources.inc

TARGETNAME=vssln$(D)
TARGETTYPE=DYNLINK
TARGETEXT=dll
TARGETPATH=$(VSBUILT)\$(BUILT_FLAVOR)\bin\$(TARGET_DIRECTORY)\vspkgs
TARGETPATHUI=$(VSBUILT)\$(BUILT_FLAVOR)\bin\$(TARGET_DIRECTORY)\vspkgs\$(CODEPAGE)
TARGETPATHLIB=$(O)

UMTYPE=windows
NO_NTDLL=1
USE_MSVCRT=1
USE_STL=1
USE_ATL=1

COFFBASE=vssln
DLLDEF=vssln.def
DLLENTRY=_DllMainCRTStartup

DELAYLOAD=comdlg32.dll

LINK_MAP_FILE=1

PASS0_HEADERDIR=$(VSBUILT)\$(BUILT_FLAVOR)\inc\$(TARGET_DIRECTORY)
PASS0_TLBDIR=$(O)
PASS0_DLLDATADIR=$(O)
PASS0_PROXYDIR=$(O)
PASS0_SOURCEDIR=$(O)

#UNDONE: workaround for Alpha compiler 6.0 compiler bug
!if "$(PROCESSOR_ARCHITECTURE)"=="ALPHA"
MSC_WARNING_LEVEL=/W3
!endif

# MIDL only finds TLBs in the current directory or the path...
PATH=$(PATH);$(PASS0_TLBDIR);$(VSBUILT)\$(BUILT_FLAVOR)\bin\$(TARGET_DIRECTORY);$(EXTERNAL_ROOT)\tlb

LIBRARIAN_FLAGS=$(LIBRARIAN_FLAGS) -ignore:4006

USER_C_DEFINES=-D_MT -D_DLL -D_ATL_STATIC_REGISTRY -DPROXY_CLSID_IS={0x9e46ed68,0x42b7,0x11d2,{0x8a,0x59,0x0,0x0,0xf8,0x5,0x8e,0x9d}} -D_ATL_NO_ATTRIBUTES
USER_INCLUDES=$(O);..\..\internal\inc;$(COMMON_ROOT)\idl\vs;$(COMMON_ROOT)\idl\vsee;..\..\..\inc
CTC_INCLUDES=$(CTC_INCLUDES);..\..\internal\inc

SOURCES= \
	vssln.idl \


