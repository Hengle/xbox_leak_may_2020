	// IJavaParameterContext interface

	[
		uuid(503894F0-C594-11D0-A8DD-00A0C921A4D2),
		dual,
		hidden,
		restricted
	]
	interface IJavaParameterContext : IDispatch
	{
	    [propget] HRESULT Methods ([out, retval] IJavaMembers **ppMembers);
	    [propget] HRESULT ParameterCount ([out, retval] long *piCount);
	    [propget] HRESULT CurrentParameter ([out, retval] long *piCurParm);
	    HRESULT GetContextStream ([out] long *piStart, long *piLength);
	
	    // Update the context using iPos as focal point.  This will rebuild
	    // state if necessary (i.e., the buffer has changed).  Return values:
	    //  S_OK        - Context was updated, and something is different
	    //                  (could be different current parameter, new set of
	    //                  methods, or even a different call)
	    //  S_FALSE     - Context is no different
	    //  E_FAIL      - iPos has no meaning in the context (note that this
	    //                  does NOT invalidate the context -- for example,
	    //                  Update (iPos, PCM_PARENTCALL) might return E_FAIL
	    //                  but that doesn't mean the tip should go away.
	    // E_INVALIDARG - if mode is PCM_CHILDCALL or PCM_PARENTCALL and iPos
	    //                  is not the same position as that used to create
	    //                  or last update this context.
	    HRESULT Update ([in] long iPos, [in] ParameterContextMode mode);
	}


