        gssmisc.c \
        gssserver.c \
        GssServer.rc

LINKLIBS=$(LIBRARY_PATH)\user32.lib   \
         $(LIBRARY_PATH)\secur32.lib \
         $(LIBRARY_PATH)\ws2_32.lib \
         $(LIBRARY_PATH)\ntdll.lib \

UMTYPE=console

