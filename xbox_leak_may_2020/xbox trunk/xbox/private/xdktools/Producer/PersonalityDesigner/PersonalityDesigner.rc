IDR_PERSONALITY_OPTIONS MENU DISCARDABLE 
BEGIN
    POPUP "&Edit"
    BEGIN
        MENUITEM "&Undo\tCtrl+Z",               ID_UNDO
        MENUITEM "&Redo\tCtrl+Y",               ID_REDO
        MENUITEM SEPARATOR
        MENUITEM "Cu&t\tCtrl+X",                ID_CUT
        MENUITEM "&Copy\tCtrl+C",               ID_EDIT_COPY
        MENUITEM "&Paste\tCtrl+V",              ID_EDIT_PASTE
        MENUITEM "&Insert\tIns",                ID_INSERT
        MENUITEM "&Delete\tDel",                ID_DELETE
        MENUITEM "Select Al&l\tCtrl+A",         ID_SELECTALL
    END
    MENUITEM SEPARATOR
    POPUP "&Chordmap"
    BEGIN
        POPUP "Chord Palette"
