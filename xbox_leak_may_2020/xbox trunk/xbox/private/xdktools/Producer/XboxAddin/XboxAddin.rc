IDR_SEGMENT_MENU MENU DISCARDABLE 
BEGIN
    POPUP "."
    BEGIN
        MENUITEM "&Boundary Settings...",       ID_BOUNDARY_SETTINGS, GRAYED
        POPUP "Use &Audiopath"
        BEGIN
            MENUITEM SEPARATOR
            MENUITEM "Standard 3D",                 ID__USEAUDIOPATH_STANDARD3DDRY

            MENUITEM "Standard Dynamic Mono",       ID__USEAUDIOPATH_STANDARDMONO

            MENUITEM "Standard Shared Stereo",      ID__USEAUDIOPATH_STANDARDSHAREDSTEREO

            MENUITEM "Standard Shared Stereo && Reverb", 
                                                    ID__USEAUDIOPATH_STANDARDSTEREOREVERB

            MENUITEM "Standard MixBin Quad",        ID__USEAUDIOPATH_STANDARDQUAD

            MENUITEM "Standard MixBin Quad + Music Reverb, Chorus", 
                                                    ID__USEAUDIOPATH_STANDARDQUADMUSIC

            MENUITEM "Standard MixBin Quad + Env Reverb", 
                                                    ID__USEAUDIOPATH_STANDARDQUADENV

            MENUITEM "Standard MixBin 5.1",         ID__USEAUDIOPATH_STANDARD51

            MENUITEM "Standard MixBin 5.1 + Music Reverb, Chorus", 
                                                    ID__USEAUDIOPATH_STANDARD51MUSIC

            MENUITEM "Standard MixBin 5.1 + Env Reverb", 
                                                    ID__USEAUDIOPATH_STANDARD51ENV

            MENUITEM "Standard MixBin Stereo + Effects", 
                                                    ID__USEAUDIOPATH_STANDARDSTEREOEFFECT

        END
        MENUITEM "&Insert Segments...",         ID_INSERT_SEGMENTS, GRAYED
        MENUITEM "&Delete Segment",             ID_DELETE_SEGMENT
    END
END

IDR_XBOXADDIN_MENU MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Open...",                    ID_FILE_OPEN, GRAYED
        MENUITEM "&Save",                       ID_FILE_SAVE, GRAYED
        MENUITEM "Save &As...",                 ID_FILE_SAVE_AS, GRAYED
        MENUITEM "&Close",                      ID_FILE_CLOSE
    END
    POPUP "&Synchronize"
    BEGIN
        MENUITEM "&All Files",                  ID_SYNCHRONIZE_ALL, GRAYED
        MENUITEM "C&hanged Files",              ID_SYNCHRONIZE_AUTO, GRAYED
    END
    POPUP "&Xbox"
    BEGIN
        MENUITEM "&Connect",                    ID_CONNECT
        MENUITEM "&Disconnect",                 ID_DISCONNECT, GRAYED
        MENUITEM "Set &Target Directory...",    ID_SET_TARGET_DIRECTORY
        MENUITEM "&Panic",                      ID_XBOX_PANIC, GRAYED
        MENUITEM "Set &Xbox Name",              ID_SET_XBOX_NAME
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About...",                   ID_APP_ABOUT
    END
END

IDR_AUDIOPATH_MENU MENU DISCARDABLE 
BEGIN
    POPUP "."
    BEGIN
        POPUP "Create &Standard"
        BEGIN
            MENUITEM "3D",                          ID_CREATE_STANDARD3D
            MENUITEM "Dynamic Mono",                ID_CREATE_STANDARDMONO
            MENUITEM "Shared Stereo",               ID_CREATE_STANDARDSHAREDSTEREO

            MENUITEM "Shared Stereo && Reverb",     ID_CREATE_STANDARDSTEREOREVERB

            MENUITEM "MixBin Quad",                 ID_CREATE_STANDARDMIXBINQUAD

            MENUITEM "MixBin Quad + Music Reverb, Chorus", 
                                                    ID_CREATE_STANDARDMIXBINQUADMUSIC

            MENUITEM "MixBin Quad + Env Reverb",    ID_CREATE_STANDARDMIXBINQUADENV

            MENUITEM "MixBin 5.1",                  ID_CREATE_STANDARDMIXBIN51

            MENUITEM "MixBin 5.1 + Music Reverb, Chorus", 
                                                    ID_CREATE_STANDARDMIXBIN51MUSIC

            MENUITEM "MixBin 5.1 + Env Reverb",     ID_CREATE_STANDARDMIXBIN51ENV

            MENUITEM "MixBin Stereo + Effects",     ID_CREATE_STANDARDSTEREOEFFECTS

        END
        MENUITEM "&Set Default",                ID_SET_DEFAULT
        MENUITEM "&Delete Audiopath",           ID_DELETE_AUDIOPATH
    END
END


