// This is a template for a multi-modal property page - there are three modes:
// Xbox Not Locked, Locked But the user doesn't have permission to manage,
// the user has permission to manage.
//
// This is created from one template as the mode can change while the page is displayed:
// the user can lock an unlocked box, the user can enter a password to gain temporary manage
// permission.
//
IDD_CONSOLE_SECURITY DIALOGEX DISCARDABLE  0, 0, 227, 215
STYLE DS_MODALFRAME |(DS_SETFONT|DS_FIXEDSYS)| DS_NOIDLEMSG | DS_3DLOOK | WS_POPUP | WS_CAPTION
CAPTION "Security"
FONT 8, "MS Shell Dlg"
BEGIN
    // These represent the full UI with manage permissions.
    LTEXT           "Machine &names:",IDC_SECURITY_MACHINES,7,7,105,8
    CONTROL         "",IDC_SECURITY_USERLIST,"SysListView32",
                    LVS_REPORT | LVS_NOCOLUMNHEADER | LVS_NOSORTHEADER | WS_TABSTOP |
                    WS_VSCROLL | LVS_SINGLESEL | LVS_SHOWSELALWAYS | LVS_SORTASCENDING, 7,17,213,66,
                    WS_EX_CLIENTEDGE
    PUSHBUTTON      "A&dd...",IDC_SECURITY_ADD,116,87,50,14
    PUSHBUTTON      "&Remove",IDC_SECURITY_REMOVE,170,87,50,14
    LTEXT           "",IDC_SECURITY_ACCESS_TEXT,7,107,105,8
    CONTROL         "",IDC_SECURITY_ACCESS,"SysListView32",
                    LVS_REPORT | LVS_NOCOLUMNHEADER | LVS_NOSORTHEADER | WS_TABSTOP, 7,117,213,72,
                    WS_EX_CLIENTEDGE
    PUSHBUTTON      "&Unlock",IDC_SECURITY_UNLOCK,7,194,65,14
    PUSHBUTTON      "&Change Password",IDC_SECURITY_CHANGE_PASSWORD,77,194,65,14

    // If there are no Access permssions - the code hides
    // all of the above, except IDC_CONSOLE_ACCESS, and shows the following:
    LTEXT           "This machine does not have manage permission.  Enter the the Adminstrator Password and click Manage to alter permissions.",
                    IDC_SECURITY_MANAGE_TEXT,7,7,210,8
    LTEXT           "&Password: ", IDC_SECURITY_PASSWORD_TEXT, 7, 20, 50, 8
    EDITTEXT        IDC_SECURITY_PASSWORD_EDIT,61,18,100,12,ES_AUTOHSCROLL|ES_PASSWORD
    PUSHBUTTON      "&Manage",IDC_SECURITY_MANAGE_BUTTON,165,17,55,14

    // If the box is not locked, all of the above is hidden, and only the following
    // is shown.
    LTEXT           "This Xbox Development Kit is not locked.  Click Lock to lock it.", IDC_SECURITY_UNLOCKED_TEXT,30,30,180,28
    PUSHBUTTON      "&Lock",IDC_SECURITY_LOCK_BUTTON,30,61,55,14
END

